{
  "extends": "/config/secrets.json",
  "utc": true,
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "4g"
    },
    "visualElements": true,
    "iterations": 5,
    "browser": "chrome",
    "video": true,
    "visualMetrics": true,

    "videoParams": {
      "thumbsize": 600
    },
    "chrome": {
      "collectConsoleLog": true,
      "enableTraceScreenshots": true
    },
    "firefox": {
      "bidihar": true,
      "disableBrowsertimeExtension": true
     }
  },
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "slug": "firstViewDocker",
  "copyLatestFilesToBase": true,
  "copyLatestFilesToBaseGraphiteNamespace": true,
  "graphite": {
    "namespace": "sitespeed_io.desktop",
    "addSlugToKey": true,
    "messages": ["pageSummary", "run"]
  },
  "thirdParty": {
    "cpu": true
  }
}
