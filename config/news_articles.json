{
  "extends": "./config/desktop.json",
  "axe": {
    "enable": true
  },
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    }
    },
  "sustainable": {
    "enable": true
  },
  "slug": "newsArticles",
  "copyLatestFilesToBase": true,
  "graphite": {
    "annotationRetentionMinutes": 10,
    "namespace": "sitespeed_io.desktopFirstView",
    "skipSummary": true,
    "addSlugToKey": true
  },
  "thirdParty": {
    "cpu": false
  }
}
